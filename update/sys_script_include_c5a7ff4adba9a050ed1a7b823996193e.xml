        <api_name>x_58872_needit.validatePhoneNumber</api_name>
        <caller_access/>
        <client_callable>true</client_callable>
        <description>on demand script include to validate the phone number</description>
        <name>validatePhoneNumber</name>
        <script><![CDATA[function validatePhoneNumber(phneNumber){
	phneNumber = phneNumber + '';
	var isValidPhone = phneNumber.match(/[2-9]{1}\d{2}/);
	if(isValidPhone){
		return true;
	}
	else{
		return false;
	}
}]]></script>
